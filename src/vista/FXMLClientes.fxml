      <Label layoutX="102.0" layoutY="38.0" text="Buscar " />
      <TableView fx:id="tblClientes" layoutX="97.0" layoutY="99.0" onMouseClicked="#accClickMouse" prefHeight="202.0" prefWidth="407.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colNombres" prefWidth="75.0" text="Nombres" />
            <TableColumn fx:id="colApellidos" prefWidth="99.0" text="Apellidos" />
            <TableColumn fx:id="colCorreo" prefWidth="148.0" text="Correo" />
        </columns>
      </TableView>
      <Button fx:id="btnAgregar" layoutX="369.0" layoutY="313.0" mnemonicParsing="false" onAction="#accBtnAgregar" text="Agregar" />
      <Button fx:id="btnCancelar" layoutX="442.0" layoutY="313.0" mnemonicParsing="false" onAction="#accBtnCancelar" text="Cancelar" />
   </children>
</AnchorPane>

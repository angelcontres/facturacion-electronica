<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.FXMLUsuariosController">
   <children>
      <TableView fx:id="tblUsuarios" layoutX="68.0" layoutY="89.0" onMouseClicked="#onMouseClick" prefHeight="245.0" prefWidth="460.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="57.0" text="ID" />
          <TableColumn fx:id="colPerfil" prefWidth="93.0" text="Perfil" />
            <TableColumn fx:id="colNombres" prefWidth="110.0" text="Nombres" />
            <TableColumn fx:id="colUsuarios" prefWidth="109.0" text="Usuario" />
            <TableColumn fx:id="colEstado" prefWidth="78.0" text="Estado" />
        </columns>
      </TableView>
      <TextField fx:id="txtBuscar" layoutX="141.0" layoutY="39.0" prefHeight="25.0" prefWidth="319.0" promptText="Buscar usuario por nombre, por cedula" />
      <Label layoutX="68.0" layoutY="39.0" prefHeight="25.0" prefWidth="62.0" text="Buscar" />
      <Button fx:id="btnAgregar" layoutX="348.0" layoutY="347.0" mnemonicParsing="false" onAction="#accBtnAgregar" text="Agregar nuevo" />
      <Button fx:id="btnCancelar" layoutX="460.0" layoutY="347.0" mnemonicParsing="false" onAction="#accBtnCancelar" prefHeight="25.0" prefWidth="68.0" text="Cancelar" />
   </children>
</AnchorPane>
